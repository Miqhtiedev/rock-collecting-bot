generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Server {
  guildId      String  @id
  prefix       String  @default("!")
  joinMessage  String?
  joinChannel  String?
  joinRoleId   String?
  leaveMessage String?
  leaveChannel String?
}
